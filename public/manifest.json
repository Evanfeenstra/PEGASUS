{
  "manifest_version": 2,

  "name": "Pegasus",
  "description": "IOTA wallet and marketplace",
  "version": "0.0.1",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "permissions": [
    "storage"
  ],
  "background": {
    "scripts": [
      "background/background.js"
    ]
  },
  "icons": { 
    "64": "./material/logo/pegasus-64.png",
    "128": "./material/logo/pegasus-128.png" 
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"

}
